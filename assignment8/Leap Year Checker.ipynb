    "\n",
    "year=int(input(\"Enter a year: \"))\n",
    "if year%400==0:\n",
    "    print(\"Leap Year\")\n",
    "elif year%4==0 and year%100!=0:\n",
    "    print(\"Leap Year\")\n",
    "else:\n",
    "    print(\"Not a Leap Year\")\n"
